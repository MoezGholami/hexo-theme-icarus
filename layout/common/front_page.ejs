<article id="<%= page.layout %>-<%= page.slug %>" class="article article-type-<%= page.layout %><%= (page.direction && page.direction.toLowerCase() === 'rtl' ? ' rtl' : '') %>" itemscope itemprop="blogPost">
    <div class="article-inner">
        <div class="article-entry" itemprop="articleBody">
        <%- page.content %>
        </div>
    </div>
</article>
